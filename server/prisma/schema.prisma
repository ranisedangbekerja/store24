generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Item {
  item_Id      String   @id @default(uuid()) @db.Uuid
  name        String   @db.VarChar(100)
  quantity    Int     
  date        DateTime @db.Date 

  @@map("item")
}
